<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#e9af30" />
            <stroke android:width="1dp" android:color="#e9af30" />
            <corners android:radius="1dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="#e9af30" android:startColor="#e9af30" />
            <stroke android:width="1dp" android:color="#e9af30" />
            <corners android:radius="1dp" />
        </shape>
    </item>
</selector>