<!DOCTYPE html>
<meta charset="utf-8">
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>JustMoney @ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- BootStrap-->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <!-- App -->
    <link rel="stylesheet" href="~/app/css/app.css" />
    <!-- Style -->
    <link rel="stylesheet" type="text/css" href="~/app/css/style.css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.1/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">

    <!-- Date Picker -->
    <link rel="stylesheet" type="text/css" href="~/app/css/datepickk.min.css">
    <script src="~/app/js/datepickk.min.js"></script>
</head>
<body ng-app="justmoney" ng-controller='AppCtrl' ng-init="app.selectedPage=''">

    <!-- HEADER -->
    <div class="header">
        <a href="http://www.justmoney.es/justmoney2/wordpress/" data-ps2id-api="true" class="header-justmoney-img"><img src="/app/img/menu-barra-1-con-logo-.png" width="100%" height="100%"></a>
        <section id="menu" style="margin-top: -15px; display: block !important;">
            @*<a href="http://www.justmoney.es/justmoney2/wordpress/#que-es" data-ps2id-api="true" class="headerLink headerLink1">¿Qué es?</a>
            <a href="http://www.justmoney.es/justmoney2/wordpress/#ventajas" data-ps2id-api="true" class="headerLink">Ventajas</a>
            <a href="http://www.justmoney.es/justmoney2/wordpress/#productos" data-ps2id-api="true" class="headerLink">Productos</a>
            <a href="http://www.justmoney.es/justmoney2/wordpress/#corporate" data-ps2id-api="true" class="headerLink">Corporate</a>
            <a href="http://www.justmoney.es/justmoney2/wordpress/#contacto" data-ps2id-api="true" class="headerLink">Contacto</a>*@
            @*<a href="#" class="mi-cuenta-img" data-ps2id-api="true"><img src="/app/img/mi-cuenta-icono.png" width="175" height="auto" style="margin-left: 30px;"></a>*@
            @*<span style="width: 825px; height: 1px; display: inline-block;">&nbsp;</span>*@
			<xp-logout>
				<a href="#" class="logout-img" data-ps2id-api="true" ng-if="app.selectedPage!=''" ng-click="logout()">
					<img src="/app/img/desconectar.png" class="desconectar-img" width="197" height="auto" style="margin-right: 30px;margin-left: auto;display: table;">
				</a>
			</xp-logout>
        </section>

        <button style="display: none !important;" class="navbar-toggler" type="button" data-toggle="collapse" aria-label="Toggle navigation" ng-click="collapsableMenu_opened = !collapsableMenu_opened" ng-disabled="_windowState.isBusy">
            <i class="fa fa-bars navbar-toggler-icon" aria-hidden="true"></i>
        </button>

        <div class="collapsableMenu" ng-style="{'opacity': collapsableMenu_opened ? '1' : '0', 'height': !collapsableMenu_opened ? '0' : 'unset'}">
            <ul id="nav" class="navbar-nav animated">
                <li id="menu-item-319" class="menuItem"><a class="nav-link _mPS2id-h" title="¿Qué es?" href="http://www.justmoney.es/justmoney2/wordpress/#que-es" data-ps2id-api="true" ng-click="collapsableMenu_opened=false">¿Qué es?</a></li>
                <li id="menu-item-320" class="menuItem"><a class="nav-link _mPS2id-h" title="Ventajas" href="http://www.justmoney.es/justmoney2/wordpress/#ventajas" data-ps2id-api="true" ng-click="collapsableMenu_opened=false">Ventajas</a></li>
                <li id="menu-item-321" class="menuItem"><a class="nav-link _mPS2id-h" title="Productos" href="http://www.justmoney.es/justmoney2/wordpress/#productos" data-ps2id-api="true" ng-click="collapsableMenu_opened=false">Productos</a></li>
                <li id="menu-item-753" class="menuItem"><a class="nav-link _mPS2id-h" title="Corporate" href="http://www.justmoney.es/justmoney2/wordpress/#corporate" data-ps2id-api="true" ng-click="collapsableMenu_opened=false">Corporate</a></li>
                <li id="menu-item-322" class="menuItem"><a class="nav-link _mPS2id-h" title="Contacto" href="http://www.justmoney.es/justmoney2/wordpress/#contacto" data-ps2id-api="true" ng-click="collapsableMenu_opened=false">Contacto</a></li>
            </ul>
        </div>
    </div>

    <div class="leftSiteLinksOuter" ng-show="app.selectedPage!=''" ng-class="{'inlineblockElement': app.selectedPage!=''}">
        <div class="leftSiteLinksInner">
            <a class="leftSiteLink" ng-class="{'leftSiteLinkActive': app.selectedPage=='prepaidcard'}" ui-sref="prepaidcard" xp-navigate>Mi cuenta</a>
            <a class="leftSiteLink" ng-class="{'leftSiteLinkActive': app.selectedPage=='prepaidcardlog'}" ui-sref="prepaidcardlog" xp-navigate>Historial de transacciones</a>
            <a class="leftSiteLink" ng-class="{'leftSiteLinkActive': app.selectedPage=='prepaidcardupdate'}" ui-sref="prepaidcardupdate" xp-navigate>Editar detalles</a>
            <a class="leftSiteLink" ng-class="{'leftSiteLinkActive': app.selectedPage=='prepaidcardloadfunds'}" ui-sref="prepaidcardloadfunds" xp-navigate>Cargar fondos</a>
            <a class="leftSiteLink" ng-class="{'leftSiteLinkActive': app.selectedPage=='prepaidcardsharefunds'}" ui-sref="prepaidcardsharefunds" xp-navigate>Compartir fondos</a>
			<a class="leftSiteLink" ng-class="{'leftSiteLinkActive': app.selectedPage=='prepaidcardaddcard'}" ui-sref="prepaidcardaddcard" xp-navigate>Solicitar / Registrar tarjeta autorizada</a>
            <a class="leftSiteLink" ng-class="{'leftSiteLinkActive': app.selectedPage=='prepaidcardenabledisable'}" ui-sref="prepaidcardenabledisable" xp-navigate>Bloquear / Desbloquear</a>
            <!--br />
            <a class="leftSiteLink" ui-sref="prepaidcardcreate" xp-navigate>(TEST) Crear usuario + tarjeta</a-->
        </div>

        <img src="/app/img/logo-fondo-intranet.png" width="100%">
    </div>

    <div class="separator"></div>

    <div ng-class="{'rightSiteOuter_full': app.selectedPage=='', 'rightSiteOuter': app.selectedPage!=''}" ng-cloak>
        <div class="rightSideMiCuenta">
            <ui-view></ui-view>
        </div>
    </div>
       
    <!-- JQuery -->
    @*<script src='http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js'></!--script>*@
    <!-- BootStrap -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <!-- Angularjs -->
    <script src="~/bower_components/angular/angular.min.js"></script>
    <script src="~/bower_components/angular-ui-router/release/angular-ui-router.min.js"></script>
    <!-- App -->
    <script src="~/app/js/app.js"></script>
    <script src="~/app/js/app.router.js"></script>
    <!-- Xp -->
    <script src="~/app/js/xp.communication.js"></script>
    <script src="~/app/js/xp.authentication.js"></script>
    <script src="~/app/js/xp.js"></script>
    <!-- Moment -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>
</body>
</html>