@using PayIn.Application.Dto.Payments.Arguments;
@using PayIn.Web.App.Factories;
@using Xp.Common;
@using (Html.xpPut(
				EntranceTypeFactory.VisibilityName,
				EntranceTypeFactory.VisibilityApi,
				"",
				"resources.entranceType.visibilityEntranceType"
			))
{
	<xp-get data-api="@EntranceTypeFactory.VisibilityApi">
		<div class="panel-body">
			<div class="col-md-12">
				<div class="row">
					<div class="row">
						@(Html.ArgumentFor<EntranceTypeVisibilityArguments>(x => x.Visibility))
					</div>
				</div>
			</div>
		</div>
	</xp-get>
}
