@*@using PayIn.Application.Dto.Transport.Results.TransportOperation;
@using PayIn.Common.Resources;
@using PayIn.Web.App.Factories;
@using Xp.Common;
@using PayIn.Common;
@using PayIn.Domain.Security;
@{
	var readAll = GreyListFactory.ReadAll("{{item.uid}}");

}
@using (Html.xpPost(
							GreyListFactory.ModifyFieldName,
							GreyListFactory.ModifyFieldApi,
							"",
							"resources.transportControl.getAll",
							isTitleActive: true,
							init: "scope.arguments.id = params.id;scope.arguments.modifyValues = [];scope.arguments.uid=params.uid;scope.arguments.modifyCard=false;"
						))
{
	<xp-get data-api="@TransportOperationFactory.DetailsChargeApi">
		<div class="navbar">
			<div>
				<div class="navbar-inner">
					<div class="col-md-8">
						<p class="pull-left" style="margin-left:1em"><span translate="resources.transportLog.update">Última actualización: </span> <b>{{temp.date | xpDateTime}}</b></p>
					</div>
					<div class="col-md-4">
						<p>
							<span class="label bg-success" tooltip="Correcto" data-ng-show="temp.ticketPay == true" style="color:#FFF">PAGADO</span>
							<span class="label bg-danger" tooltip="{{temp.ticketPayError || 'Sin Pagar'}}" data-ng-hide="temp.ticketPay == true">SIN PAGAR</span>
							<span class="label bg-success" tooltip="Aplicado" data-ng-show="temp.rechargedApplied == true" style="color:#FFF">APLICADO</span>
