				<tab heading="Lista gris" active="steps.step5">
					<div class="col col-md-7">
						<span translate="resources.transportLog.infoCard">Tarjeta con UID </span>{{temp.uid}}
					</div>
					<div class="col col-md-5">
						<input type="text" class="input-sm form-control" placeholder="Filtrar" data-ng-model="filter">
					</div>
					<table class="table table-hover" data-ng-show="arguments.greyList.length > 0">
						<thead>
							<tr>
								<th translate="resources.transportGreyList.date">Fecha</th>
								<th translate="resources.transportGreyList.action">Acción</th>
								<th translate="resources.transportGreyList.value">Campo (Valor)</th>
								<th translate="resources.transportGreyList.resolve">Resuelto (Fecha)</th>
								<th translate="resources.transportGreyList.source">Origen</th>
								<th translate="resources.transportGreyList.operationNumber">Nº operación</th>
								<th>&nbsp;</th>
							</tr>
						</thead>
						<tbody>
							<tr ng-repeat="item in arguments.greyList | filter:filter">
								<td>{{item.registrationDate | xpDateTime}}</td>
								<td>{{item.action | eigeAction}} ({{item.action}})</td>
								<td><span data-ng-show="item.field">{{item.field}} ({{item.newValue}})</span></td>
								<td>{{item.resolved}} <span data-ng-show="item.resolved == true">({{item.resolutionDate}})</span></td>
								<td><span data-ng-show="item.source == 1">SIGA>punT</span><span data-ng-show="item.source == 2">Pay[in]</span></td>
								<td>{{item.operationNumber}}</td>
								<td>
									<span data-ng-show="item.resolved == true"> &nbsp;</span>
									<span data-ng-hide="item.resolved == true">
										@XpColumns.IconColumn(
										XpIcons.Trash,
										popupUrl: GreyListFactory.Delete,
										id: "{{item.id}}"
									   )
									</span>
								</td>
							</tr>
						</tbody>
					</table>
					<h3 data-ng-hide="arguments.greyList.length > 0"> No hay datos de esta tarjeta en la lista gris</h3>
				</tab>
				<tab heading="Lista blanca" active="steps.step6">
					<div class="col col-md-7">
						<span translate="resources.transportLog.infoCard">Tarjeta con UID </span>{{temp.uid}}
					</div>
					<div class="col col-md-5">
						<input type="text" class="input-sm form-control" placeholder="Filtrar" data-ng-model="filterWhite">
					</div>
					<table class="table table-hover" data-ng-show="arguments.whiteList.length > 0">
						<thead>
							<tr>
								<th translate="resources.transportWhiteList.inclusionDate">Fecha</th>
								<th translate="resources.transportWhiteList.exclusionDate">Fecha</th>
								<th translate="resources.transportWhiteList.titleType">Tipo de título</th>
								<th translate="resources.transportWhiteList.operationType">Tipo de operación</th>
								<th translate="resources.transportWhiteList.amount">Cantidad</th>
								<th translate="resources.transportWhiteList.operationNumber">Nº operación</th>
							</tr>
						</thead>
						<tbody>
							<tr ng-repeat="item in arguments.whiteList | filter:filterWhite">
								<td>{{item.registrationDate | xpDateTime}}</td>
								<td>{{item.action | eigeAction}} ({{item.action}})</td>
								<td><span data-ng-show="item.field">{{item.field}} ({{item.newValue}})</span></td>
								<td>{{item.resolved}} <span data-ng-show="item.resolved == true">({{item.resolutionDate}})</span></td>
								<td><span data-ng-show="item.source == 1">SIGA>punT</span><span data-ng-show="item.source == 2">Pay[in]</span></td>
								<td>{{item.operationNumber}}</td>
							</tr>
						</tbody>
					</table>
					<h3 data-ng-hide="arguments.whiteList.length > 0"> No hay datos de esta tarjeta en la lista blanca</h3>
				</tab>
				<tab heading="Dispositivo" active="steps.step7">
					<table class="table table-hover" data-ng-show="temp.device.length > 0">
						<thead>
							<tr>
								<th translate="resources.transportLog.deviceManufacturer">Fabricante</th>
								<th translate="resources.transportLog.deviceModel">Model</th>
								<th translate="resources.transportLog.devicePlatform">Platform</th>
								<th translate="resources.transportLog.deviceUuid">UUID</th>
								<th translate="resources.transportLog.deviceSerial">Nº Serie</th>
								<th translate="resources.transportLog.deviceOperator">Operador</th>
								<th translate="resources.transportLog.deviceImei">IMEI</th>
								<th translate="resources.transportLog.deviceMac">MAC</th>
							</tr>
						</thead>
						<tbody>
							<tr data-ng-repeat="device in temp.device">
								<td>{{device.manufacturer}}</td>
								<td>{{device.model}}</td>
								<td>{{device.platform}} {{device.version}}</td>
								<td>{{device.uuid}}</td>
								<td>{{device.serial}}</td>
								<td>{{device.operator}}</td>
								<td>{{device.imei}}</td>
								<td>{{device.mac}}</td>
							</tr>
						</tbody>
					</table>
					<h3 data-ng-hide="temp.device.length > 0"> No hay datos sobre este dispositivo</h3>
				</tab>
			</tabset>
		</div>
	</xp-get>
