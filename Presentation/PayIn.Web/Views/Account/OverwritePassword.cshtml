@using PayIn.Application.Dto.Security.Arguments;
@using PayIn.Web.App.Factories;

@using (Html.xpPopupPut(
    AccountFactory.OverwritePasswordName,
    AccountFactory.OverwritePasswordApi,
    "",
    "resources.account.overwritePassword"
))
{
    @(Html.ArgumentFor<AccountOverwritePasswordArguments>(x => x.Password, width: 6))
    @(Html.ArgumentFor<AccountOverwritePasswordArguments>(x => x.ConfirmPassword, width: 6))
}
